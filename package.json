    "@tailwindcss/vite": "^4.1.18",
    "alpinejs": "^3.15.4",
    "astro": "^5.16.15",
    "astro-compress": "^2.3.9",
    "astro-icon": "^1.1.5",
    "astro-robots-txt": "^1.0.0",
